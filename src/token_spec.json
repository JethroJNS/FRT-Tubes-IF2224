{
  "version": "1.0",
  "precedence": [
    "ASSIGN_OPERATOR",
    "RANGE_OPERATOR",
    "RELATIONAL_OPERATOR_SYMBOL",
    "ARITHMETIC_OPERATOR_SYMBOL",
    "PUNCTUATION"
  ],
  "types": [
    "KEYWORD",
    "LOGICAL_OPERATOR",
    "IDENTIFIER",
    "NUMBER",
    "CHAR_LITERAL",
    "STRING_LITERAL",
    "ARITHMETIC_OPERATOR",
    "RELATIONAL_OPERATOR",
    "ASSIGN_OPERATOR",
    "SEMICOLON",
    "COMMA",
    "COLON",
    "DOT",
    "LPARENTHESIS",
    "RPARENTHESIS",
    "LBRACKET",
    "RBRACKET",
    "RANGE_OPERATOR",
    "COMMENT_START",
    "COMMENT_END",
    "WHITESPACE",
    "UNKNOWN"
  ],
  "keywords": [
    "program","var","begin","end","if","then","else","while","do","for","to","downto",
    "integer","real","boolean","char","array","of","procedure","function","const","type"
  ],
  "logical_operators": ["and","or","not"],
  "arithmetic_operators_symbol": ["+","-","*","/"],
  "arithmetic_operators_word": ["div","mod"],
  "relational_operators_symbol": ["=","<>","<","<=",">",">="],
  "assign_operator": [":="],
  "punctuation": {
    "SEMICOLON": ";",
    "COMMA": ",",
    "COLON": ":",
    "DOT": ".",
    "LPARENTHESIS": "(",
    "RPARENTHESIS": ")",
    "LBRACKET": "[",
    "RBRACKET": "]"
  },
  "range_operator": [".."],
  "comments": {
    "brace": { "start": "{", "end": "}" },
    "paren_star": { "start": "(*", "end": "*)" }
  },
  "patterns": {
    "IDENTIFIER": "^[A-Za-z_][A-Za-z0-9_]*$",
    "NUMBER": "^(?:[0-9]+(?:\\.[0-9]+)?)$",
    "CHAR_LITERAL": "^'([^'\\n]|'')'$",
    "STRING_LITERAL": "^'([^'\\n]|'')*'$",
    "WHITESPACE": "^[\\t\\r\\n\\f ]+$"
  },
  "classification_rules": [
    "Exact-match ke 'keywords' → KEYWORD",
    "Exact-match ke 'logical_operators' → LOGICAL_OPERATOR",
    "Exact-match ke 'arithmetic_operators_word' → ARITHMETIC_OPERATOR",
    "Exact-match ke 'relational_operators_symbol' → RELATIONAL_OPERATOR",
    "Exact-match ':=' → ASSIGN_OPERATOR",
    "Exact-match '..' → RANGE_OPERATOR",
    "Exact-match punctuation map → tipe sesuai key",
    "Regex NUMBER → NUMBER",
    "Regex CHAR_LITERAL → CHAR_LITERAL",
    "Regex STRING_LITERAL → STRING_LITERAL",
    "Regex IDENTIFIER → IDENTIFIER",
    "Regex WHITESPACE → WHITESPACE",
    "Lainnya → UNKNOWN"
  ],
  "output_format": "TOKEN_TYPE(value)"
}